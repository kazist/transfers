    "query": {
        "from": {
            "table": "Transfers\\Transfers\\Code\\Tables\\Transfers",
            "alias": "tt",
            "name": "#__transfers_transfers"
        },
        "select": [
            "tt.id",
            "tt.origin_user_id",
            "uu.name AS origin_user_id_name",
            "tt.target_user_id",
            "uu_478.name AS target_user_id_name",
            "tt.amount",
            "tt.gateway_id",
            "pg.long_name AS gateway_id_long_name",
            "tt.currency",
            "tt.token",
            "tt.description",
            "tt.params",
            "tt.is_canceled",
            "tt.created_by",
            "uu_9933.name AS created_by_name",
            "tt.date_created",
            "tt.modified_by",
            "uu_9313.name AS modified_by_name",
            "tt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "tt.origin_user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_478",
                "name": "#__users_users",
                "on": "tt.target_user_id = uu_478.id"
            },
            {
                "table": "Payments\\Gateways\\Code\\Tables\\Gateways",
                "alias": "pg",
                "name": "#__payments_gateways",
                "on": "tt.gateway_id = pg.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_9933",
                "name": "#__users_users",
                "on": "tt.created_by = uu_9933.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_9313",
                "name": "#__users_users",
                "on": "tt.modified_by = uu_9313.id"
            }
        ]
    }
